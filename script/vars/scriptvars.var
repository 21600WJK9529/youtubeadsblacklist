  DOCTOSPITOUT="$REPODIR"domainlist.txt
  HOSTSFORMALSPITOUT="$REPODIR"hosts.txt
  TEMPFILE="$SCRIPTDIR"tempfile.temp

  SCRIPTDIR="$REPODIR"script/
