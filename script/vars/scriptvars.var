      HOSTSFORMALSPITOUTOLD="$LASTRUNDIR"hosts.lastrun.txt
      ROOTSUBSBADLIST="$LASTRUNDIR"badroots.txt
      ROOTSUBSOLDLIST="$LASTRUNDIR"rootsubs.lastrun.txt     
    
    VARSDIR="$SCRIPTDIR"vars/
      ROOTSUBSLIST="$VARSDIR"rootsubs.txt
      MAINREADMEDEFAULT="$VARSDIR"README.md
